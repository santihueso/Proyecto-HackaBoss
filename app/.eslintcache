ink>\n      <p>{name}</p>\n      {data.length > 0 ? (\n        <List array={data}></List>\n      ) : (\n        <p>No hay libros en esta categoría.</p>\n      )}\n    </div>\n  );\n};\nconst ViewBook = () => {\n  let { idBook, name, id, kind } = useParams();\n  const [valoration, setValoration] = useState(false);\n  const history = useHistory();\n  const [data] = useFetchData(\n    `http://localhost:${port}/beginning/category/${idBook}`\n  );\n  const [category] = useFetchData(\n    `http://localhost:${port}/beginning/categories`\n  );\n\n  let buttons;\n  let nameLink;\n  if (kind) {\n    if (kind === \"favorites\") {\n      nameLink = \"Favoritos\";\n      buttons = (\n        <div>\n          <ButtonDelete\n            idBook={idBook}\n            to={\"favorite\"}\n            rout={\"favorites\"}\n          ></ButtonDelete>\n          <ButtonPurchaseFavoriteReserved\n            idBook={idBook}\n            to={\"buy\"}\n            name={\"Comprar\"}\n            rout={\"purchase\"}\n          ></ButtonPurchaseFavoriteReserved>\n        </div>\n      );\n    } else if (kind === \"purchase\") {\n      nameLink = \"Comprados\";\n      buttons = (\n        <div>\n          <button onClick={() => setValoration(true)}>Entregado</button>\n          {valoration ? (\n            <FormValoration idBook={idBook}></FormValoration>\n          ) : null}\n        </div>\n      );\n    } else if (kind === \"offers\") {\n      nameLink = \"Notificaciones\";\n      buttons = (\n        <div>\n          <button>Ver</button>\n        </div>\n      );\n    } else if (kind === \"reserved\") {\n      nameLink = \"Reservados\";\n      buttons = (\n        <div>\n          <ButtonDelete\n            idBook={idBook}\n            to={\"reservation\"}\n            rout={\"reserved\"}\n          ></ButtonDelete>\n          <ButtonBuyWithReserved idBook={idBook}></ButtonBuyWithReserved>\n        </div>\n      );\n    } else if (kind === \"toSell\") {\n      nameLink = \"En venta\";\n      buttons = (\n        <div>\n          <button>Editar</button>\n          <ButtonDelete\n            idBook={idBook}\n            to={\"to\"}\n            rout={\"toSell\"}\n          ></ButtonDelete>\n        </div>\n      );\n    }\n  } else {\n    buttons = (\n      <div>\n        <ButtonPurchaseFavoriteReserved\n          idBook={idBook}\n          to={\"favorite\"}\n          name={\"Favorito\"}\n          rout={\"favorites\"}\n        ></ButtonPurchaseFavoriteReserved>\n        <ButtonPurchaseFavoriteReserved\n          idBook={idBook}\n          to={\"reservation\"}\n          name={\"Reservar\"}\n          rout={\"reserved\"}\n        ></ButtonPurchaseFavoriteReserved>\n        <ButtonPurchaseFavoriteReserved\n          idBook={idBook}\n          to={\"buy\"}\n          name={\"Comprar\"}\n          rout={\"purchase\"}\n        ></ButtonPurchaseFavoriteReserved>\n      </div>\n    );\n  }\n\n  const book = data.map((e) => {\n    const findCategory = category.find(\n      (element) => element.id_category === e.category\n    );\n\n    const nameCategory = () =>\n      findCategory ? findCategory.category_name : null;\n    return (\n      <div key={e.id_product}>\n        <nav>\n          <Link to=\"/principal\">Principal ˃ </Link>\n          {name !== \"ultimos\" ? (\n            <Link to={`/principal/category/${id}/${name}`}>{name}</Link>\n          ) : (\n            <p>Últimos libros</p>\n          )}\n          {kind ? (\n            <Link to={`/principal/profile/list/${kind}`}>{nameLink}</Link>\n          ) : null}\n          <p> Libro</p>\n        </nav>\n\n        <img\n          src={`http://localhost:${port}/uploads/${e.photoFront}`}\n          alt=\"portada\"\n          style={{ maxWidth: 80 }}\n        ></img>\n        <img\n          src={`http://localhost:${port}/uploads/${e.photoBack}`}\n          alt=\"portada\"\n          style={{ maxWidth: 80 }}\n        ></img>\n        <div>\n          <h1>{e.productName}</h1>\n          <p>{e.author}</p>\n          <p>{e.bookLanguage}</p>\n          {buttons}\n          <p>{e.descriptionProduct}</p>\n          <p>{e.price}</p>\n          {kind ? (\n            <Link\n              to={`/principal/category/${\n                e.category\n              }/${nameCategory()}/book/${idBook}/user/${e.seller}`}\n            >\n              Perfil\n            </Link>\n          ) : (\n            <Link\n              to={`/principal/category/${id}/${name}/book/${idBook}/user/${e.seller}`}\n            >\n              Perfil\n            </Link>\n          )}\n        </div>\n      </div>\n    );\n  });\n  return <div>{book}</div>;\n};\n\nexport { BookOfKindCategory, ViewBook };\n","/home/santiago/Codigo/Proyecto-Recybook/Proyecto-HackaBoss/app/src/components/profile/ProfileInside.js",["91","92"],"import React, { useState } from \"react\";\nimport { UseLabelInput } from \"../signin-login/UseForm\";\nimport { port } from \"../principalPage/Principal\";\nimport { useHistory, Link } from \"react-router-dom\";\nimport { useFetchAuth } from \"../useFetch/useFetchAuth\";\n\nconst CreateProfile = () => {\n  const [auth] = useState(JSON.parse(localStorage.getItem(\"auth\")) || \"\");\n  const [dataUser, setDataUser] = useFetchAuth(\n    `http://localhost:${port}/login/user/profile`,\n    auth\n  );\n  let [img, setImg] = useState(null);\n  let [name, setName] = useState(\"\");\n  let [description, setDescription] = useState(\"\");\n  let [city, setCity] = useState(\"\");\n  let [cp, setCp] = useState(\"\");\n  const [onImg, setOnImg] = useState(false);\n  const [onName, setOnName] = useState(false);\n  const [onDescription, setOnDescription] = useState(false);\n  const [onCity, setOnCity] = useState(false);\n  const [onCp, setOnCp] = useState(false);\n  const history = useHistory();\n  const user = dataUser.map((user) => {\n    const handlSubmit = async (e) => {\n      e.preventDefault();\n      const formData = new FormData();\n      if (onImg) {\n        formData.append(\"photo\", img, img.name);\n      } else {\n        img = user.photo;\n        formData.append(\"photo\", img);\n      }\n      if (!name) {\n        name = user.username;\n      }\n      if (!description) {\n        description = user.descriptionUser;\n      }\n      if (!city) {\n        city = user.city;\n      }\n      if (!cp) {\n        cp = user.postalCode;\n      }\n\n      formData.append(\"username\", name);\n      formData.append(\"descriptionUser\", description);\n      formData.append(\"city\", city);\n      formData.append(\"postalCode\", cp);\n      const res = await fetch(`http://localhost:${port}/user/editUser`, {\n        method: \"PUT\",\n        headers: {\n          Authorization: auth,\n        },\n        body: formData,\n      });\n      if (res.status > 300) {\n        console.warn(\"error\", res);\n      }\n      history.push(\"/principal/profile\");\n    };\n\n    return (\n      <div key={user.id_user}>\n        <form onSubmit={handlSubmit}>\n          <div>\n            <nav>\n              <Link to=\"/principal\">Principal ˃ </Link>\n              <Link to=\"/principal/profile\">Perfil</Link>\n            </nav>\n            <label htmlFor=\"userImage\"></label>\n            <input\n              id=\"userImage\"\n              type=\"file\"\n              onChange={(e) => [setOnImg(true), setImg(e.target.files[0])]}\n              accept=\"image/*\"\n            ></input>\n          </div>\n          <div>\n            <label htmlFor=\"name\"></label>\n            <input\n              id=\"name\"\n              value={onName ? name : user.username}\n              type=\"text\"\n              placeholder=\"nombre\"\n              onChange={(e) => [setOnName(true), setName(e.target.value)]}\n            ></input>\n          </div>\n          <div>\n            <label htmlFor=\"description\"></label>\n            <input\n              id=\"description\"\n              value={onDescription ? description : user.descriptionUser}\n              type=\"text\"\n              placeholder=\"descripción\"\n              onChange={(e) => [\n                setOnDescription(true),\n                setDescription(e.target.value),\n              ]}\n            ></input>\n          </div>\n          <div>\n            <label htmlFor=\"city\"></label>\n            <input\n              id=\"city\"\n              value={onCity ? city : user.city}\n              type=\"text\"\n              placeholder=\"ciudad\"\n              onChange={(e) => [setOnCity(true), setCity(e.target.value)]}\n            ></input>\n          </div>\n          <div>\n            <label htmlFor=\"cp\"></label>\n            <input\n              id=\"cp\"\n              value={onCp ? cp : user.postalCode}\n              type=\"number\"\n              placeholder=\"codigo postal\"\n              onChange={(e) => [setOnCp(true), setCp(e.target.value)]}\n            ></input>\n          </div>\n\n          <input type=\"submit\" value=\"submit\"></input>\n        </form>\n        <button onClick={() => history.push(\"/principal/profile\")}>\n          Cancelar\n        </button>\n      </div>\n    );\n  });\n  return user;\n};\n\nconst ProfileUserInside = () => {\n  const [auth] = useState(JSON.parse(localStorage.getItem(\"auth\")) || \"\");\n  const [dataUser] = useFetchAuth(\n    `http://localhost:${port}/login/user/profile`,\n    auth\n  );\n\n  const showProfile = dataUser.map((e) => {\n    const url = `http://localhost:${port}/uploads/${e.photo}`;\n    return (\n      <div key={e.id_user}>\n        <nav>\n          <Link to=\"/principal\">Principal ˃ </Link>\n          <Link to=\"/principal/profile/edit\">Editar</Link>\n        </nav>\n\n        <img src={url} alt=\"avatar\" style={{ maxWidth: 80 }}></img>\n        <p>{e.username}</p>\n        <p>{e.descriptionUser}</p>\n        <p>{e.postalCode}</p>\n        <nav>\n          <Link to=\"/principal/profile/list/favorites\">Favoritos</Link>\n          <Link to=\"/principal/profile/list/offers\">Notificaciones</Link>\n          <Link to=\"/principal/profile/list/purchase\">Comprados</Link>\n          <Link to=\"/principal/profile/list/toSell\">En venta</Link>\n          <Link to=\"/principal/profile/list/reserved\">Reservados</Link>\n        </nav>\n      </div>\n    );\n  });\n  return showProfile;\n};\n\nexport { ProfileUserInside, CreateProfile };\n","/home/santiago/Codigo/Proyecto-Recybook/Proyecto-HackaBoss/app/src/components/profile/ListBooksUser.js",[],"/home/santiago/Codigo/Proyecto-Recybook/Proyecto-HackaBoss/app/src/components/signin-login/Signin.js",[],"/home/santiago/Codigo/Proyecto-Recybook/Proyecto-HackaBoss/app/src/components/signin-login/Login.js",[],"/home/santiago/Codigo/Proyecto-Recybook/Proyecto-HackaBoss/app/src/components/useFetch/useFetchAuth.js",[],"/home/santiago/Codigo/Proyecto-Recybook/Proyecto-HackaBoss/app/src/components/useFetch/useFetchData.js",[],"/home/santiago/Codigo/Proyecto-Recybook/Proyecto-HackaBoss/app/src/components/signin-login/UseForm.js",[],"/home/santiago/Codigo/Proyecto-Recybook/Proyecto-HackaBoss/app/src/components/buttons/Buttons.js",[],"/home/santiago/Codigo/Proyecto-Recybook/Proyecto-HackaBoss/app/src/components/buttons/FormValoration.js",[],"/home/santiago/Codigo/Proyecto-Recybook/Proyecto-HackaBoss/app/src/components/changePassword/changePassword.js",["93","94","95"],{"ruleId":"96","severity":1,"message":"97","line":1,"column":17,"nodeType":"98","messageId":"99","endLine":1,"endColumn":26},{"ruleId":"96","severity":1,"message":"100","line":21,"column":16,"nodeType":"98","messageId":"99","endLine":21,"endColumn":23},{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"96","severity":1,"message":"105","line":3,"column":10,"nodeType":"98","messageId":"99","endLine":3,"endColumn":22},{"ruleId":"96","severity":1,"message":"106","line":34,"column":9,"nodeType":"98","messageId":"99","endLine":34,"endColumn":16},{"ruleId":"96","severity":1,"message":"107","line":2,"column":10,"nodeType":